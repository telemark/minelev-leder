{
  "name": "minelev-leder",
  "version": "1.10.3",
  "description": "Admin frontend for MinElev",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.11.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "deploy": "npm test && now && now alias",
    "start": "node standalone.js",
    "dev": "NODE_ENV=development node standalone.js",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "tfk",
    "Digitalisering",
    "MinElev",
    "Leder"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "Joan Jett",
    "versionVideoUrl": "https://www.youtube.com/watch?v=46G1nRdCOio",
    "systemName": "MinElev - Leder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/minelev-leder.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/minelev-leder/issues"
  },
  "homepage": "https://github.com/telemark/minelev-leder#readme",
  "now": {
    "alias": [
      "leder.minelev.no"
    ],
    "type": "npm",
    "engines": {
      "node": "8.11.1"
    },
    "dotenv": "production.env"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.1",
    "nsp": "3.2.1",
    "nyc": "12.0.2",
    "standard": "11.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "dotenv": "5.0.1",
    "good": "7.3.0",
    "good-console": "7.1.0",
    "good-squeeze": "5.1.0",
    "handlebars": "4.0.11",
    "hapi": "16.6.3",
    "hapi-auth-cookie": "7.1.0",
    "hapi-auth-jwt2": "7.4.1",
    "inert": "4.2.1",
    "jsonwebtoken": "8.2.1",
    "simple-encryptor": "1.3.0",
    "tfk-saksbehandling-schools-info": "2.0.12",
    "vision": "4.1.1",
    "winston": "2.4.2",
    "winston-papertrail": "1.0.5",
    "yar": "8.1.2"
  }
}
